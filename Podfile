# Uncomment the next line to define a global platform for your project
platform :ios, '12.0'
workspace 'RestauRoulette.xcworkspace'
use_frameworks!

def Rx_pods
  pod 'ReSwift'
  pod 'RxSwift' 
end

target 'Infrastructure' do
  	project 'Infrastructure/Infrastructure'  
  	workspace 'RestauRoulette.xcworkspace'

    pod 'ReSwift'
    pod 'RxSwift'
end

target 'PositionInfrastructure' do
  	project 'PositionInfrastructure/PositionInfrastructure'
  	workspace 'RestauRoulette.xcworkspace'

    pod 'ReSwift'
    pod 'RxSwift'
end


target 'NetworkInfrastructure' do
  	project 'NetworkInfrastructure/NetworkInfrastructure'  
  	workspace 'RestauRoulette.xcworkspace'

    pod 'ReSwift'
    pod 'RxSwift'
end

target 'Application' do
  	project 'Application/Application'  
  	workspace 'RestauRoulette.xcworkspace'

    pod 'ReSwift'
    pod 'RxSwift'
end

target 'RestauRoulette' do
  project 'RestauRoulette'  
  workspace 'RestauRoulette.xcworkspace'

  pod 'ReSwift'
  pod 'RxSwift'
end
